<?xml version="1.0" encoding="utf-8"?>
<openbox_menu>
	<menu execute="obm-xdg" id="xdg-menu" label="Applications"/>
	<menu id="root-menu" label="OpenBox 3">
		<item label="Terminal">
			<action name="Execute">
				<execute>
					urxvt
				</execute>
			</action>
		</item>
		<item label="Home">
			<action name="Execute">
				<execute>
					thunar
				</execute>
			</action>
		</item>
		<separator/>
		<item label="Browser">
			<action name="Execute">
				<execute>
					chromium --user-data-dir=/home/josh/.chromium
				</execute>
			</action>
		</item>
		<item label="VLC">
			<action name="Execute">
				<execute>
					vlc
				</execute>
			</action>
		</item>
		<item label="Mixer">
			<action name="Execute">
				<execute>
					urxvt -e alsamixer
				</execute>
			</action>
		</item>
		<separator/>
		<menu id="xdg-menu"/>
		<separator/>
		<menu id="40" label="OpenBox">
			<menu id="client-list-menu"/>
			<item label="Reconfigure">
				<action name="Reconfigure"/>
			</item>
			<separator/>
			<item label="Exit">
				<action name="Exit"/>
			</item>
		</menu>
	</menu>
</openbox_menu>
